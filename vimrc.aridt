source $(vimrc systemPath)
nnoremap <silent> $(sendKey default) :exe 'sil w !python3 -S $(sendblock)' winnr() 'default' line('.') line('.')<CR>
nnoremap <silent> $(sendKey alternate) :exe 'sil w !python3 -S $(sendblock)' winnr() 'alternate' line('.') line('.')<CR>
vnoremap <silent> $(sendKey default) :<C-U>exe 'sil w !python3 -S $(sendblock)' winnr() 'default' line("'<") line("'>")<CR>
vnoremap <silent> $(sendKey alternate) :<C-U>exe 'sil w !python3 -S $(sendblock)' winnr() 'alternate' line("'<") line("'>")<CR>
source $(vimrc userPath)
